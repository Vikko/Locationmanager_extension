<div data-role="page" data-add-back-btn="false">

  <div data-role="header" data-position="inline">
    <h1><%= @swing.lat_start %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="arrow-l" data-direction="reverse">
      Back
    </a>
    <a href="<%= url_for :action => :edit, :id => @swing.object %>" class="ui-btn-right" data-icon="star">
      Edit
    </a>
  </div>

  <div data-role="content">
    <ul data-role="listview">
      
          <li>
            <div class="itemLabel">LatStart</div>
            <div class="itemValue"><%= @swing.lat_start %></div>
          </li>
      
          <li>
            <div class="itemLabel">LongStart</div>
            <div class="itemValue"><%= @swing.long_start %></div>
          </li>
      
          <li>
            <div class="itemLabel">LatEnd</div>
            <div class="itemValue"><%= @swing.lat_end %></div>
          </li>
      
          <li>
            <div class="itemLabel">LongEnd</div>
            <div class="itemValue"><%= @swing.long_end %></div>
          </li>
      
          <li>
            <div class="itemLabel">Heading</div>
            <div class="itemValue"><%= @swing.heading %></div>
          </li>
      
          <li>
            <div class="itemLabel">Distance</div>
            <div class="itemValue"><%= @swing.distance %></div>
          </li>
      
    </ul>
  </div>

</div>
